package demo;

public class Demo {
	public static void main(String[] args) {
		      
		
	  String s = "java in the master";
	  
	  int total_length = s.length();
	  
	  String s1 = s.replaceAll("a", "");
	  
	  int totalAfterRemove = s1.length();
	  
	  int result = total_length - totalAfterRemove;
	  
	  System.out.println(result);
	  
	    
	}
	}
