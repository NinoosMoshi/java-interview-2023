package demo;

import java.util.ArrayList;

public class Demo {

	public static void main(String[] args) {
		
		// ninos , nahrain, matthew
		
		ArrayList<String> s = new ArrayList<>();
		s.add("ninos");
		s.add("nahrain");
		s.add("matthew");
		System.out.println(s);
		
		for(int i=s.size(); i > 0; i--) {
			System.out.println(s);
		}
		
		StringBuffer sb = new StringBuffer();
		System.out.println();
		
		
		
		
		
     
     
     
     
     
     
	}

}
