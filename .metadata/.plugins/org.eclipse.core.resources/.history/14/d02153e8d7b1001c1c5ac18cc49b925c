package demo;

import java.util.ArrayList;
import java.util.List;

public class Demo {

	public static void main(String[] args) {
		
      List<Integer> l = new ArrayList<>();   
      l.add(1);
      l.add(2);
      l.add(3);
      
      for(int i=0; i< l.size(); i++) {
    	  System.out.print(l);
      }
     
     
     
     
     
     
     
	}

}
