package demo;

import java.util.ArrayList;

public class Demo {

	public static void main(String[] args) {
		
      ArrayList<String> l = new ArrayList<>();   
      l.add("1");
      l.add("2");
      l.add("3");
      
      for(int i=0; i< l.size(); i++) {
    	  System.out.print(l[i]);
      }
     
     
     
     
     
     
     
	}

}
